{
  "name": "mapbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "apollo": "vue-cli-service apollo:watch",
    "apollo:run": "vue-cli-service apollo:run",
    "dev": "nodemon ./src/server.js",
    "start": "node server.js"
  },
  "dependencies": {
    "express": "^4.16.4",
    "firebase": "^5.5.6",
    "graphql-type-json": "^0.2.1",
    "save-dev": "^2.0.0",
    "slugify": "^1.3.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "uws": "^99.0.0",
    "vue": "^2.5.17",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-image-compressor": "^1.0.3",
    "vue-image-upload-resize": "^2.0.3",
    "vue-native-notification": "^1.0.5",
    "vue-router": "^3.0.1",
    "vue-socket.io": "^2.1.1-b",
    "vue-socket.io-extended": "^3.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "eslint-plugin-graphql": "^2.1.1",
    "graphql-tag": "^2.9.0",
    "nodemon": "^1.18.9",
    "vue-cli-plugin-apollo": "^0.19.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "graphql/template-strings": [
        "error",
        {
          "env": "literal",
          "projectName": "app"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "graphql"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
